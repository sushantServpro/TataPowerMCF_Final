<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt" height="100%"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Panel >
		<content>
			<Panel>
				<html:div class="go-green_intro">
					<html:div class="row intro_head">
						<html:div class="col-xs-12">
							<html:div class="knw_cen_bg border_rad_icon">
								<html:div class="row icon_row_flex">
									<html:div class="col-xs-2">
										<html:image src="images/icon_knowledge.png" alt="icon_knowledge" class="img-responsive"></html:image>
									</html:div>
									<html:div class="col-xs-10">
										<html:h4>How To Read Bill</html:h4>
									</html:div>
								</html:div>
							</html:div>
						</html:div>
					</html:div>
				</html:div>
				<html:div class="row">
					<html:div class="newtabflash">
						<IconTabBar id="idIconTabBarNoIcons" expanded="{device>/isNoPhone}" class="sapUiResponsiveContentPadding">
							<items>
								<IconTabFilter text="Bill Part 1">
									<html:div class="col-md-12">
										<html:object>
											<html:embed src="flash/KnowledgeCentreTataPowerConsumerCarePortal1.swf" width="100%" height="500"></html:embed>
										</html:object>
									</html:div>
								</IconTabFilter>
								<IconTabFilter text="Bill Part 2">
									<html:div class="col-md-12">
										<html:object>
											<html:embed src="flash/KnowledgeCentreTataPowerConsumerCarePortal2.swf" width="100%" height="500"></html:embed>
										</html:object>
									</html:div>
								</IconTabFilter>
							</items>
						</IconTabBar>
					</html:div>
				</html:div>
			</Panel>
		</content>
	</Panel>
</core:FragmentDefinition>