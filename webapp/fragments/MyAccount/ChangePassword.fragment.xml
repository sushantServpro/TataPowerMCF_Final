<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt" height="100%"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Panel >
		<content>
			<Panel>
				<html:div class="container-fluid">
					<html:div class="row middlebox share_feedbk">
						<html:div class="content">
							<html:div class="row">
								<html:div class="col-sm-7 col-md-6">
									<html:div class="row">
										<html:div class="col-sm-6">
											<html:div class="row bill_payment">
												<html:div class="col-xs-12">
													<GenericTile class="tileLayout" press="press">
														<TileContent>
															<content>
																<html:div class="row">
																	<html:div class="col-xs-3">
																		<html:image src="images/myac_online_bill.png" alt="speakers" class="img-repsonsive"></html:image>
																	</html:div>
																	<html:div class="col-xs-9">
																		<html:span>quick bill payment</html:span>
																	</html:div>
																</html:div>
															</content>
														</TileContent>
													</GenericTile>
												</html:div>
											</html:div>
										</html:div>
										<html:div class="col-sm-6">
											<html:div class="row your_bill">
												<html:div class="col-xs-12">
													<GenericTile class="tileLayout" press="press">
														<TileContent>
															<content>
																<html:div class="row">
																	<html:div class="col-xs-3">
																		<html:image src="images/myac_pay_bill.png" alt="myac_pay_bill" class="img-responsive"></html:image>
																	</html:div>
																	<html:div class="col-xs-9">
																		<html:span>pay your bill here</html:span>
																	</html:div>
																</html:div>
															</content>
														</TileContent>
													</GenericTile>
												</html:div>
											</html:div>
										</html:div>
									</html:div>
								</html:div>
								<html:div class="col-sm-5 col-md-6 counters">
									<html:div class="row">
										<html:div class="col-xs-4 col-sm-4 text-center">
											<html:span>Consumer Number</html:span>
											<html:h4>1234</html:h4>
										</html:div>
										<html:div class="col-xs-4 col-sm-4 text-center acc_no">
											<html:span>Account Number</html:span>
											<html:h4>1234</html:h4>
										</html:div>
										<html:div class="col-xs-4 col-sm-4 text-center">
											<html:span>Meter Number</html:span>
											<html:h4>1234</html:h4>
										</html:div>
									</html:div>
								</html:div>
							</html:div>
						</html:div>
					</html:div>
					<html:div class="acc_change_password">
						<html:div class="row">
							<html:h4>change password</html:h4>
							<html:p>
								<html:em>If you'd registered in the portal earlier and do not remember the password, please key in your consumer no. and the password will
									be sent to your email id as well as mobile number registered with us.</html:em>
							</html:p>
							<html:p>
								<html:em>Please get your New Consumer Number from What's New section and enter here</html:em>
							</html:p>
						</html:div>
						<html:div class="row">
							<html:div class="col-xs-12">
								<html:div class="row chng_row">
									<html:div class="col-sm-4 col-sm-offset-4">
										<Label text="Consumer Number" labelFor="ncon"/>
										<Input type="Number" id="ncon" required="true"/>
									</html:div>
								</html:div>
								<html:div class="row chng_row">
									<html:div class="col-sm-4 col-sm-offset-4">
										<html:div class="row">
											<html:div class="col-sm-6">
												<Button text="GENERATE PASSWORD" press="onPress" class="switchcenter form-control"
													ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
											</html:div>
											<html:div class="col-sm-6">
												<Button text="CANCEL" press="onPress" class="switchcenter form-control" ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
											</html:div>
										</html:div>
									</html:div>
								</html:div>
							</html:div>
						</html:div>
					</html:div>
				</html:div>
			</Panel>
		</content>
	</Panel>
</core:FragmentDefinition>