<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt" height="100%"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Panel >
		<content>
			<Panel>
				<html:div class="container-fluid">
					<html:div class="row middlebox share_feedbk">
						<html:div class="content">
							<html:div class="row">
								<html:div class="col-sm-7 col-md-6">
									<html:div class="row">
										<html:div class="col-sm-6">
											<html:div class="row bill_payment">
												<html:div class="col-xs-12">
													<GenericTile class="tileLayout" press="press">
														<TileContent>
															<content>
																<html:div class="row">
																	<html:div class="col-xs-3">
																		<html:image src="images/myac_online_bill.png" alt="speakers" class="img-repsonsive"></html:image>
																	</html:div>
																	<html:div class="col-xs-9">
																		<html:span>quick bill payment</html:span>
																	</html:div>
																</html:div>
															</content>
														</TileContent>
													</GenericTile>
												</html:div>
											</html:div>
										</html:div>
										<html:div class="col-sm-6">
											<html:div class="row your_bill">
												<html:div class="col-xs-12">
													<GenericTile class="tileLayout" press="press">
														<TileContent>
															<content>
																<html:div class="row">
																	<html:div class="col-xs-3">
																		<html:image src="images/myac_pay_bill.png" alt="myac_pay_bill" class="img-repsonsive"></html:image>
																	</html:div>
																	<html:div class="col-xs-9">
																		<html:span>pay your bill here</html:span>
																	</html:div>
																</html:div>
															</content>
														</TileContent>
													</GenericTile>
												</html:div>
											</html:div>
										</html:div>
									</html:div>
								</html:div>
								<html:div class="col-sm-5 col-md-6 counters">
									<html:div class="row">
										<html:div class="col-xs-4 col-sm-4 text-center">
											<html:span>Consumer Number</html:span>
											<html:h4>1234</html:h4>
										</html:div>
										<html:div class="col-xs-4 col-sm-4 text-center acc_no">
											<html:span>Account Number</html:span>
											<html:h4>1234</html:h4>
										</html:div>
										<html:div class="col-xs-4 col-sm-4 text-center">
											<html:span>Meter Number</html:span>
											<html:h4>1234</html:h4>
										</html:div>
									</html:div>
								</html:div>
							</html:div>
						</html:div>
					</html:div>
					<html:div class="row share_ur_feed">
						<html:div>
							<html:h4>Please share your feedback</html:h4>
						</html:div>
						<html:div class="feedback_form">
							<html:h5 class="mandatory">
								<html:em>*Indicates Mandatory Fields</html:em>
							</html:h5>
							<html:div class="content">
								<html:div class="row form_row">
									<html:div class="col-sm-6">
										<Label text="Name" labelFor="name"/>
										<Input id="name" required="true" type="Text" placeholder="Enter"/>
									</html:div>
									<html:div class="col-sm-6">
										<Label text="Account Number" labelFor="acc_no"/>
										<Input type="acc_no" id="full_name" required="true" placeholder="Enter"/>
									</html:div>
								</html:div>
								<html:div class="row form_row">
									<html:div class="col-sm-6">
										<Label text="Email ID" labelFor="email_id"/>
										<Input type="Email" id="email_id" required="true" placeholder="Enter"/>
									</html:div>
									<html:div class="col-sm-6">
										<Label text="Mobile Number" labelFor="mob_no"/>
										<Input type="mob_no" id="mob_no" required="true" placeholder="Enter"/>
									</html:div>
								</html:div>
								<html:div class="feed_quetion">
									<html:h3 class="text-center">answer only 5 questions</html:h3>
									<html:div class="ques_div">
										<html:h5>"How often" do you use the Tata Power Customer Portal?</html:h5>
										<html:div class="row radioBtn">
											<html:div class="col-sm-3"><RadioButton groupName="ques1" text="Daily"/></html:div>
											<html:div class="col-sm-3"><RadioButton groupName="ques1" text="Weekly" selected="true"/></html:div>
											<html:div class="col-sm-3"><RadioButton groupName="ques1" text="Monthly"/></html:div>
											<html:div class="col-sm-3"><RadioButton groupName="ques1" text="Yearly"/></html:div>
										</html:div>
									</html:div>
									<html:div class="ques_div">
										<html:h5>Which of the "following services" of Tata Power Customer Portal you use?</html:h5>
										<html:div class="row radioBtn">
											<html:div class="col-sm-3"><CheckBox text="Bill Payments" groupName="ques2" selected="true"/></html:div>
											<html:div class="col-sm-3"><CheckBox text="View &amp; Download Bills" groupName="ques2"/></html:div>
											<html:div class="col-sm-3"><CheckBox text="Registering Complaints" groupName="ques2" selected="true"/></html:div>
											<html:div class="col-sm-3"><CheckBox text="As &amp; When required" groupName="ques2"/></html:div>
										</html:div>
										<html:div class="row radioBtn">
											<html:div class="col-sm-3"><CheckBox text="DSM Programme Registration" groupName="ques2"/></html:div>
											<html:div class="col-sm-3"><CheckBox text="Tariff Related Info." groupName="ques2" selected="true"/></html:div>
											<html:div class="col-sm-3"><CheckBox text="Seeking answers to queries" groupName="ques2"/></html:div>
											<html:div class="col-sm-3"><CheckBox text="Other" groupName="ques2" selected="true"/></html:div>
										</html:div>
										<html:div class="ques_area">
											<TextArea placeholder="Please Specify" growing="true" growingMaxLines="7" width="100%"/>
										</html:div>
									</html:div>
								</html:div>
								<html:div class="feed_rating">
									<html:div class="row feedRatingFlex">
										<html:div class="col-sm-6">
											<html:div class="ques_div">
												<html:h5>Kindly rate us on the Look &amp; Feel of our existing Customer Portal?</html:h5>
												<html:div class="row">
													<html:div class="col-sm-8">
														<RatingIndicator id="rate1" maxValue="4" class="sapUiSmallMarginBottom" value="3" iconSize="22px" tooltip="Rating Tooltip"/>
													</html:div>
													<html:div class="col-sm-4 uRated">You Rated:
														<html:span>3</html:span>
													</html:div>
												</html:div>
											</html:div>
										</html:div>
										<html:div class="col-sm-6">
											<html:div class="ques_div">
												<html:h5>Kindly rate us on the Ease of Navigation on Tata Power Customer Portal?</html:h5>
												<html:div class="row">
													<html:div class="col-sm-8">
														<RatingIndicator id="rate2" maxValue="4" class="sapUiSmallMarginBottom" value="3" iconSize="22px" tooltip="Rating Tooltip"/>
													</html:div>
													<html:div class="col-sm-4 uRated">You Rated:
														<html:span>3</html:span>
													</html:div>
												</html:div>
											</html:div>
										</html:div>
									</html:div>
								</html:div>
								<html:div class="feed_sugg">
									<html:div class="ques_div">
										<html:h5>We shall be keen to hear from you any suggestion?</html:h5>
										<html:div class="ques_area">
											<TextArea placeholder="Please enter your suggestion" growing="true" growingMaxLines="7" width="100%"/>
										</html:div>
									</html:div>
								</html:div>
								<html:div>
									<html:div class="col-md-2 col-md-offset-5 text-center">
										<Button text="SUBMIT" press="onPress" class="switchcenter rpfbtn" ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
									</html:div>
								</html:div>
							</html:div>
						</html:div>
					</html:div>
				</html:div>
			</Panel>
		</content>
	</Panel>
</core:FragmentDefinition>