<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt" height="100%"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Panel class="headerMainPanel">
		<content>
			<html:div class="header_one">
				<html:nav class="navbar navbar-default">
					<html:div class="container-fluid">
						<html:div class="navbar-header">
							<html:image src="images/tata_logo.png" alt="tata_logo" class="img-responsive"></html:image>
						</html:div>
						<html:div class="navbar-header pull-right">
							<html:image src="images/tata_emblem.png" alt="tata_emblem" class="img-responsive"></html:image>
						</html:div>
						<html:ul class="nav navbar-nav navbar-right search_list">
							<html:li>
								<SearchField class="sapUiSmallMargin"/>
							</html:li>
						</html:ul>
						<html:ul class="nav navbar-nav navbar-right lang_link">
							<html:li>
								<html:a href="#" title="facebook" target="_blank">
									<html:image src="images/language.png"></html:image>
								</html:a>
							</html:li>
							<html:li>
								<MenuButton text="English" buttonMode="Split" class="btn_lang">
