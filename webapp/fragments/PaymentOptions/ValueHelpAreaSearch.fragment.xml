<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="No Data Found"
		title="Area List"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		close="_handleValueHelpClose"
		items="{path: 'Area>/results'}">
		<ColumnListItem>
			<cells>
				<Text text="{Area>ZpcArea}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="Area" />
				</header>
			</Column>
			
			
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>