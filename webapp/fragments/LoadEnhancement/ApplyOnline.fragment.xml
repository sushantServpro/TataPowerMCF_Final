<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt" height="100%"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Panel >
		<content>
			<Panel>
				<html:h5>Apply Online</html:h5>
				<html:div class="apply">
					<html:div class="row">
						<html:div class=" col-md-2 tata1">
							<html:image src="images/tata_logo_white.png" alt="tata_logo" class="img-responsive"></html:image>
						</html:div>
						<html:div class=" col-md-1 pull-right tata2">
							<html:image src="images/tata_emblem_white.png" alt="tata_logo" class="img-responsive"></html:image>
						</html:div>
					</html:div>
				</html:div>
				<html:h6 class="indicate">*Indicates Mandatory Fields</html:h6>
				<html:div class="detailrequire">
					<html:div class="detailspremises">
						<html:h5 class="indicate2"></html:h5>
						<html:hr/>
					</html:div>
					<html:div class="boxpad">
						<html:div class="row">
							<html:div class=" col-md-2">
								<RadioButton groupName="Groupselect" id="RBAPPL1" text="Load Enhancement" select="OnApplicationChange" selected="true"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton groupName="Groupselect" id="RBAPPL2" text="Load Reduction" select="OnApplicationChange"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton groupName="Groupselect" id="RBAPPL3" text="Category Change" select="OnApplicationChange"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton groupName="Groupselect" id="RBAPPL4" text="Meter Shifting" select="OnApplicationChange"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton groupName="Groupselect" id="RBAPPL5" text="CMD Enhancement" select="OnApplicationChange"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton groupName="Groupselect" id="RBAPPL6" text="CMD Enhancement" select="OnApplicationChange"/>
							</html:div>
						</html:div>
						<html:div class="row sapUiMediumMarginTop">
							<Label class="col-md-4" text="Contract Acct:" labelFor="input-a"/>
							<html:div class="col-md-4">
								<!--<Input id="contractacctInput" required="true" type="Text" class="sapUiSmallMarginBottom" showValueHelp="true" valueHelpRequest="ValueHelpAcct" />-->
								<Input id="contractacctInput" required="true" type="Text" class="sapUiSmallMarginBottom" showValueHelp="true"
									valueHelpRequest="ValueHelpAcctsearch"/>
								<!--<Input id="TarrifId" showSuggestion="true" showValueHelp="true"
									     valueHelpRequest="handleValueHelp"
									     suggestionItems="{path:'Tariff>/results'}" 
									     value="{BillCalModel>/Tariff}" >-->
								<!--<suggestionItems>
										<core:Item text="{Contract>ContractAccountID}" key="{Contract>ContractAccountID}"/>
								</suggestionItems>-->
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Bussiness partn" enabled="false" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="bussinesspartnInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Type Of Consumer" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="typeofconInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Business Partner Name" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="bpartnernameInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="c/o" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="flatInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Street 2" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="street2Input" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Street 3" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="street3Input" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" required="true" text="City" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="cityInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" required="true" text="District" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="districtInput" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Postal Code" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="pcodeInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Land Mark" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="landmarkInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Mobile Phone" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="mphone" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" required="true" text="E-Mail Address" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="emailInput" required="true" type="Text" class="sapUiSmallMarginBottom"
								placeholder="(Email ID is required for speedy communication)"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Meter No." labelFor="input-a"/>
							<html:div class="col-md-4">
								<Select>
									<core:Item key="100" text="100"/>
									<core:Item key="200" text="200"/>
									<core:Item key="300" text="300"/>
								</Select>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Installation" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="installationInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Type Of Supply" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="typeofsupplyInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Rate Category" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="ratecategoryInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Existing Con_Load(KW)" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="existingconloadInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="0.0000000"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Existing CMD(KVA)" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="existingcmdInput" enabled="false" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="0.0000000"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Required/Additional Con_Load(KW)" labelFor="input-a" required="true"/>
							<html:div class="col-md-4"><Input id="reqconloadInput" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="0.0000000"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Required/Additional CMD(KVA)" labelFor="input-a" required="true"/>
							<html:div class="col-md-4"><Input id="reqcmdInput" required="true" type="Text" class="sapUiSmallMarginBottom" placeholder="0.0000000"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Required Rate Category" labelFor="input-a" required="true"/>
							<html:div class="col-md-4">
								<Select id="reqratecatInput" required="true" class="sapUiSmallMarginBottom">
									<core:Item key="100" text="100"/>
									<core:Item key="200" text="200"/>
									<core:Item key="300" text="300"/>
								</Select>
								<!--<Input id="reqratecatInput" required="true" type="Text" class="sapUiSmallMarginBottom"/>-->
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="New Connected Load (kW)" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="newconnectedloadkwInput" enabled="false" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="New Contract Demand (kVA)" labelFor="input-a"/>
							<html:div class="col-md-4"><Input id="newcontrctdemandInput" enabled="false" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:p class="col-md-12 text-danger">New Connected Load(kW) / New Contracted Demand (KVA) will consider for billing purpose.
							</html:p>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="Type of premises" labelFor="input-a"/>
							<html:div class="col-md-2">
								<RadioButton id="RB11" groupName="GroupA" text="Owned" selected="true"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton id="RB22" groupName="GroupA" text="Rented"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton id="RB33" groupName="GroupA" text="Lease"/>
							</html:div>
							<html:div class="col-md-2">
								<RadioButton id="RB44" groupName="GroupA" text="Other (Specify)"/>
							</html:div>
						</html:div>
						<html:div class="row powerreq">
							<html:div class="col-xs-12">
								<html:h5>Power Supply Requirement
								</html:h5>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-offset-2 col-md-3">
								<CheckBox id="LowVoltSingle" text="Low Voltage Single Phase" selected="false"/>
							</html:div>
							<html:div class="col-md-3">
								<CheckBox id="LowVoltThree" text="Low Voltage Three Phase" selected="false"/>
							</html:div>
							<html:div class="col-md-3">
								<CheckBox id="HighVolt" text="High Voltage" selected="false"/>
							</html:div>
						</html:div>
						<html:div class="row powerreq">
							<html:div class="col-xs-12">
								<html:h5>Load Details
								</html:h5>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-offset-2 col-md-3">
								<RadioButton groupName="GroupE" id="RBGRP_ID1" text="LED lights/Tube lights/Bulbs" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="RBGRP_ID2" text="Fans" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="RBGRP_ID3" text="Geysers" selected="true"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-offset-2 col-md-3">
								<RadioButton groupName="GroupE" id="RBGRP_ID4" text="Oven/Micro Wave" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="RBGRP_ID5" text="Chiller/Air Conditioners" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="RBGRP_ID6" text="Refrigerator" selected="true"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-offset-2 col-md-3">
								<RadioButton groupName="GroupE" id="RBGRP_ID7" text="Television" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="RBGRP_ID8" text="Machinery/Apparatus" selected="true"/>
							</html:div>
							<html:div class="col-md-3">
								<RadioButton groupName="GroupE" id="RBGRP_ID9" text="Miscellaneous" selected="true"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="No of Points" labelFor="input-a" required="true"/>
							<html:div class="col-md-4"><Input id="pointInput" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<Label class="col-md-4" text="W/Point" labelFor="input-a" required="true"/>
							<html:div class="col-md-4"><Input id="wattInput" type="Text" class="sapUiSmallMarginBottom"/>
							</html:div>
						</html:div>
						<html:div class="row">
							<html:div class="col-md-1">
								<Button text="Add Load Details" press="onAddLoadDetails" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>
							<html:div class="col-md-1">
								<Button text="Remove Load Details" press="onRemoveLoadDetails" class="switchcenter"
									ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
							</html:div>
						</html:div>
						<html:div class="row">
							<Table id="ApplyOlLoadTab" growing="true" noDataText="{NewConnectionView>/tableNoDataText}" growingScrollToLoad="true"
								mode="SingleSelectLeft" items="{oLoadTableModel}">
								<columns>
									<Column minScreenWidth="Desktop">
										<Text text="Apertures"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<Text text="Number Of Point"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<Text text="Wattage Per Point"/>
									</Column>
									<Column minScreenWidth="Desktop" demandPopin="true">
										<Text text="Total Wattage"/>
									</Column>
								</columns>
								<items>
									<ColumnListItem>
										<cells>
											<ObjectIdentifier title="{Apertures}"/>
											<Text text="{NoPoint}"/>
											<Text text="{WattPerPoint}"/>
											<Text text="{TotWatt}"/>
										</cells>
									</ColumnListItem>
								</items>
							</Table>
