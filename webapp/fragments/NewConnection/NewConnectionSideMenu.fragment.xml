<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:tnt="sap.tnt" height="100%"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Panel>
		<content>
			<Tree  items="{path: '/'}">
				<CustomTreeItem>
					<FlexBox alignItems="Start" width="100%">
						<items >
							<FlexBox >
								<Link text="{text}" press="handleButtonPress"/>
							</FlexBox>
						</items>
					</FlexBox>
				</CustomTreeItem>
			</Tree>
		</content>
	</Panel>
</core:FragmentDefinition>