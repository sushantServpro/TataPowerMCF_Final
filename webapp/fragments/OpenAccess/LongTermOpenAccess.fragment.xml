										</Toolbar>
									</headerToolbar>
									<columns>
										<Column>
											<Text text="Period From"/>
										</Column>
										<Column>
											<Text text="Period To"/>
										</Column>
										<Column>
											<Text text="Time From(HH)"/>
										</Column>
										<Column>
											<Text text="Time From(MM)"/>
										</Column>
										<Column>
											<Text text="Time To(HH)"/>
										</Column>
										<Column>
											<Text text="Time To(MM)"/>
										</Column>
										<Column>
											<Text text="Capacity(MW)"/>
										</Column>
									</columns>
									<items>
										<ColumnListItem>
											<cells>
												<ObjectIdentifier title="{Name}" text="{ProductId}"/>
												<Text text="{SupplierName}"/>
												<Text text="{Width} x {Depth} x {Height} {DimUnit}"/>
												<ObjectNumber number="{WeightMeasure}" unit="{WeightUnit}"
