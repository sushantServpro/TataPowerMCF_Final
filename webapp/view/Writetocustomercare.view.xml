<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="tatapower.dev.controller.Writetocustomercare"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="My Account" showNavButton="true" navButtonPress="onNavBack" showHeader="false">
				<content>
					<core:Fragment fragmentName="tatapower.dev.fragments.Main.HeaderMain" type="XML"/>
					<core:Fragment fragmentName="tatapower.dev.fragments.Main.HeaderMenu" type="XML"/>
					<html:div class="container chngHeight">
						<Panel >
							<content>
								<Panel id="pnl_15">
									<html:div class="row intro_head">
										<html:div class="col-xs-12">
											<html:div class="write_bg border_rad_icon">
												<html:div class="row icon_row_flex">
													<html:div class="col-xs-2">
														<html:image src="images/icon_write_to_cc.png" alt="icon_write_to_cc" class="img-responsive"></html:image>
													</html:div>
													<html:div class="col-xs-10">
														<html:h4>Write to Customer Care</html:h4>
													</html:div>
												</html:div>
											</html:div>
										</html:div>
									</html:div>
									<html:div class="container-fluid">
										<html:div class="share_ur_feed">
											<html:div class="row feedbk_ques">
												<html:div class="container-fluid">
													<html:div class="row quesn">
														<html:div class="ques_no">
															<html:h5>
																Based on your experience of our website, How would you like to rate on ease of website?
																<html:span class="req_star">*</html:span>
															</html:h5>
														</html:div>
														<html:div class="row ques_ans">
															<html:div class="col-sm-3">
																<RadioButton groupName="ques1" text="Very Difficult" selected="true"/>
																<html:span class="feed_img">
																	<html:image src="images/red_smile.png" alt="be_green_banner"></html:image>
																</html:span>
															</html:div>
															<html:div class="col-sm-3">
																<RadioButton groupName="ques1" text="Difficult"/>
																<html:span class="feed_img">
																	<html:image src="images/yellow_smile.png" alt="be_green_banner"></html:image>
																</html:span>
															</html:div>
															<html:div class="col-sm-3">
																<RadioButton groupName="ques1" text="Easy"/>
																<html:span class="feed_img">
																	<html:image src="images/blue_smile.png" alt="be_green_banner"></html:image>
																</html:span>
															</html:div>
															<html:div class="col-sm-3">
																<RadioButton groupName="ques1" text="Very Easy"/>
																<html:span class="feed_img">
																	<html:image src="images/green_smile.png" alt="be_green_banner"></html:image>
																</html:span>
															</html:div>
														</html:div>
													</html:div>
													<html:div class="row quesn">
														<html:div class="ques_no">
															<html:h5>
																Based on your experience of our website, How would you like to rate us on the quality of website?
																<html:span class="req_star">*</html:span>
															</html:h5>
														</html:div>
														<html:div class="row ques_ans">
															<html:div class="col-sm-3">
																<RadioButton groupName="ques2" text="Very Difficult"/>
																<html:span class="feed_img">
																	<html:image src="images/red_smile.png" alt="be_green_banner"></html:image>
																</html:span>
															</html:div>
															<html:div class="col-sm-3">
																<RadioButton groupName="ques2" text="Difficult" selected="true"/>
																<html:span class="feed_img">
																	<html:image src="images/yellow_smile.png" alt="be_green_banner"></html:image>
																</html:span>
															</html:div>
															<html:div class="col-sm-3">
																<RadioButton groupName="ques2" text="Easy"/>
																<html:span class="feed_img">
																	<html:image src="images/blue_smile.png" alt="be_green_banner"></html:image>
																</html:span>
															</html:div>
															<html:div class="col-sm-3">
																<RadioButton groupName="ques2" text="Very Easy"/>
																<html:span class="feed_img">
																	<html:image src="images/green_smile.png" alt="be_green_banner"></html:image>
																</html:span>
															</html:div>
														</html:div>
													</html:div>
												</html:div>
											</html:div>
											<html:div class="row form_row">
												<html:div class="col-sm-4">
													<Label text="Consumer Number" labelFor="consm_num"/>
													<Input id="consm_num" required="true" placeholder="Enter"/>
												</html:div>
												<html:div class="col-sm-8">
													<html:div class="feed_category">
														<Label text="Category" labelFor="mobile_num"/>
													</html:div>
													<html:div class="row cat_opt">
														<html:div class="col-sm-3">
															<RadioButton groupName="cat" text="Residential"/>
														</html:div>
														<html:div class="col-sm-3">
															<RadioButton groupName="cat" text="Commercial" selected="true"/>
														</html:div>
														<html:div class="col-sm-3">
															<RadioButton groupName="cat" text="Industrial"/>
														</html:div>
														<html:div class="col-sm-3">
															<RadioButton groupName="cat" text="Other"/>
														</html:div>
													</html:div>
												</html:div>
											</html:div>
											<html:div class="row form_row">
												<html:div class="col-md-2 col-md-offset-4">
													<Button text="SUBMIT" press="onPress" class="switchcenter form-control" ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
												</html:div>
												<html:div class="col-md-2">
													<Button text="NOT NOW" press="onPress" class="switchcenter form-control" ariaDescribedBy="defaultButtonDescription genericButtonDescription"></Button>
												</html:div>
											</html:div>
										</html:div>
									</html:div>
								</Panel>
							</content>
						</Panel>
					</html:div>
					<core:Fragment fragmentName="tatapower.dev.fragments.Main.Footer" type="XML"/>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>